{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FilteringOptions": {
    "FilterWords": [
      "spam",
      "badword",
      "test"
    ],
    "SimilarityAlgorithm": "Levenshtein",
    "SimilarityThreshold": 0.8
  }
}
